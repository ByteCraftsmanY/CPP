    "editor.formatOnSave": true,
    "editor.fontFamily": "Menlo",
    "editor.rulers": [
        80,
        100
    ],
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "editor.wordWrap": "off",
    "editor.minimap.enabled": true,
    "editor.suggestSelection": "first",
    "editor.acceptSuggestionOnEnter": "on",
    // "editor.bracketPairColorization.enabled": true,
    // "editor.guides.bracketPairs": true,
    "editor.semanticHighlighting.enabled": true,
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/**"
    ],
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0}",
    "C_Cpp.default.compilerPath": "/opt/homebrew/bin/g++-15",
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 1000,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "workbench.colorTheme": "Panda Syntax",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.startupEditor": "none",
    "explorer.confirmDelete": true,
    "explorer.confirmDragAndDrop": false,
    "search.exclude": {
        "**/build": true,
        "**/bin": true,
        "**/.git": true
    },
    "code-runner.executorMap": {
        "cpp": "cd $dir && /opt/homebrew/bin/g++-15 -std=c++17 $fileName -o '/Users/magicpin/Programs/CPP/bin/a' && '/Users/magicpin/Programs/CPP/bin/a' > '/Users/magicpin/Programs/CPP/bin/#output.txt' < '/Users/magicpin/Programs/CPP/bin/#input.txt'",
    },
    "cursor.cpp.disabledLanguages": [
        "markdown",
        "plaintext",
        "cpp"
    ],
    "[code-runner-output]": {
        "editor.fontSize": 14,
        "editor.fontFamily": "Monaco, 'Courier New', monospace",
        "editor.lineHeight": 1.2,
    }
